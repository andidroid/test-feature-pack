<module name="io.netty.netty-handler" xmlns="urn:jboss:module:1.9">
    <properties>
        <property name="jboss.api" value="private"/>
    </properties>

    <resources>
        <artifact name="${io.netty:netty-handler}"/>
    </resources>

    <dependencies>
        <module name="io.netty.netty-common"/>
        <module name="io.netty.netty-buffer"/>
        <module name="io.netty.netty-transport"/>
        <module name="io.netty.netty-transport-native-unix-common"/>
        <module name="io.netty.netty-codec"/>
        <module name="io.netty.netty-resolver"/>
        <module name="io.netty.netty-tcnative" services="import" export="true"/>
    </dependencies>

</module>
